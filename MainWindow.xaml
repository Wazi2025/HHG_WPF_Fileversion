<Window x:Class="HHG_WPF_Fileversion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HHG_WPF_Fileversion"
        mc:Ignorable="d"
        Title="MainWindow" Height="650" Width="800">
    <Grid Margin="0,0,0,-81">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="22*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="tbFirstName" Grid.Column="1" HorizontalAlignment="Left" Margin="42,77,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="194" RenderTransformOrigin="-0.114,0.406" Height="18"/>
        <TextBox x:Name="tbLastName" Grid.Column="1" HorizontalAlignment="Left" Margin="42,139,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="194" RenderTransformOrigin="-0.114,0.406" Height="18"/>
        <TextBox x:Name="tbAge" Grid.Column="1" HorizontalAlignment="Left" Margin="42,195,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="194" RenderTransformOrigin="-0.114,0.406" Height="18"/>
        <Button x:Name="btnOK" Grid.Column="1" Content="Get Quote" HorizontalAlignment="Left" Margin="177,244,0,0" VerticalAlignment="Top" Click="btnOK_Click" Height="20" Width="59" RenderTransformOrigin="0.356,0.552" IsDefault="True"/>
        <Label Grid.Column="1" Content="Age" HorizontalAlignment="Left" Margin="42,165,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.469,0.945" Height="26" Width="31"/>
        <Label Grid.Column="1" Content="Last name" HorizontalAlignment="Left" Margin="42,110,0,0" VerticalAlignment="Top" Height="26" Width="64"/>
        <Label Grid.Column="1" Content="First name" HorizontalAlignment="Left" Margin="42,46,0,0" VerticalAlignment="Top" Height="26" Width="65"/>
        <Image x:Name="image" Grid.Column="1" HorizontalAlignment="Left" Height="100" Margin="366,299,0,0" VerticalAlignment="Top" Width="100"/>
        <TextBlock x:Name="tbQuote" Grid.Column="1" HorizontalAlignment="Left" Margin="42,304,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="180" Width="419">
            <TextBlock.Style>
                <Style/>
            </TextBlock.Style>
        </TextBlock>

    </Grid>
</Window>
